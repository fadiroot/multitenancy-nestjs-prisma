generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      =  "postgresql://myUser:myPassword@db:5432/myDb?schema=public"
}

model Tenant {
  id          Int      @id @default(autoincrement())
  name        String
  domain      String   @unique
  dbName      String
  dbUser      String
  dbPassword  String
  dbPort      Int
  isUsed      Boolean
  containerId String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
